<core:FragmentDefinition height="100%" width="100%" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form">
	<Panel>
		<ObjectHeader title="{Finca}" number="{Lote}" numberUnit="">
			<ObjectAttribute text="{Finca}"/>
			<ObjectAttribute text="{Lote}"/>
		</ObjectHeader>
		<IconTabBar select="onDetailSelect" id="idIconTabBar">
			<items>
				<IconTabFilter key="UsuarioDetails" text="UsuarioDetails" icon="sap-icon://hint">
					<!--key="Detail" -->
					<content>
						<core:Fragment fragmentName="com.ataway.app.view.DetailInfoForm" type="XML"/>
					</content>
				</IconTabFilter>
			</items>
		</IconTabBar>
	</Panel>
</core:FragmentDefinition>